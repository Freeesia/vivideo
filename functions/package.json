    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.3.0",
    "snyk": "^1.996.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0",
    "tslint": "^6.0.0",
    "typescript": "~4.6.4"
  },
  "private": true,
  "snyk": true
}
