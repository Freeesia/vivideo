{
  "name": "@vivideo/functions",
  "version": "0.1.0",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w",
    "serve": "yarn build && firebase emulators:start",
